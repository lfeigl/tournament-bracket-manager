{
  "name": "tournament-bracket-manager",
  "version": "0.0.0",
  "description": "A simple manager for tournament brackets.",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "npm run generate-config && npm run build-client",
    "start": "node server/index.js",
    "generate-config": "node scripts/generate-config.js",
    "build-client": "webpack",
    "watch-client": "webpack -w",
    "watch-server": "supervisor -w server -s -pid pid.log -n error server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matze-kalle/tournament-bracket-manager.git"
  },
  "author": "Luca Feigl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matze-kalle/tournament-bracket-manager/issues"
  },
  "homepage": "https://github.com/matze-kalle/tournament-bracket-manager#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "angular": "^1.7.8",
    "angular-route": "^1.7.8",
    "body-parser": "^1.19.0",
    "bulma": "^0.7.5",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "jquery-bracket": "^0.11.1",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.7"
  },
  "devDependencies": {
    "css-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  }
}
